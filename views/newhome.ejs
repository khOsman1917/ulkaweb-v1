      function SmoothScroll(_target, _duration) {
        window.scrollTo(0, 0);

        var target = document.getElementsByClassName(_target)[0];
        var targetPos = target.getBoundingClientRect().top;
        var startPos = window.scrollY;
        var dist = targetPos - startPos;
        var startTime = null;

        var animation = function (_currentTime) {
          if (startTime == null) startTime = _currentTime;
          var timeElapsed = _currentTime - startTime;
          var run = Ease(timeElapsed, startPos, dist, _duration);
          window.scrollTo(0, run);
          if (timeElapsed < _duration) requestAnimationFrame(animation);
        };

        requestAnimationFrame(animation);
      }

      function Ease(time, startPos, dist, dur) {
        time /= dur / 2;
        if (time < 1) return dist / 2 * time * time + startPos;
        time--;
        return -dist / 2 * (time * (time - 2) - 1) + startPos;
      }



